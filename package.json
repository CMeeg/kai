{
  "name": "kai",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "config/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "nextjs-app": "pnpm --dir \"packages/kai-next-js-app\"",
    "test": "turbo run test",
    "test:coverage": "turbo run test:coverage",
    "testing-lib": "pnpm --dir \"packages/kai-delivery-testing-lib\"",
    "ts:check": "turbo run ts:check"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@7.28.0",
  "dependencies": {},
  "devDependencies": {
    "eslint-config-kai": "workspace:*",
    "prettier": "^2.8.4",
    "turbo": "^1.8.3",
    "vitest": "^0.29.2"
  }
}
